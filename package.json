{
    "name": "@ggttoo44/base-server",
    "version": "0.0.57",
    "description": "API Server used TypeScript",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "precommit": "lint-staged && npm test",
        "test": "nyc ./node_modules/.bin/mocha --require ts-node/register --recursive",
        "test:debug": "mocha --inspect --debug-brk --not-timeouts ts:ts-node/register src/**/*.spec.ts",
        "tslint": "tslint src/**/*.ts",
        "clear": "rm -rf package-lock.json && rm -rf node_modules && npm cache clean -f && npm prune &&  npm install && npm run rebuild:context",
        "start:rebuild": "npm run clear && npm run start:live",
        "duplicated": "./node_modules/.bin/jscpd --config .cpd.yaml",
        "preversion": "npm test && npm run build",
        "version": "git add -A .",
        "postversion": "git push && git push --tags",
        "build": "rm -rf lib && ./node_modules/.bin/tsc && cp -r ./src/sqlTemplate/ ./lib/sqlTemplate/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/thaiwu0107/SBaseServer.git"
    },
    "keywords": [
        "RESTful",
        "TypeScript"
    ],
    "author": "ThaiWu",
    "license": "MIT",
    "dependencies": {
        "@types/validator": "9.4.1",
        "bignumber.js": "7.2.1",
        "class-validator": "0.9.1",
        "clusterws": "^3.2.0",
        "cors": "2.8.4",
        "find-up": "3.0.0",
        "grpc-caller": "^0.9.0",
        "heartbeats": "^5.0.1",
        "inversify": "^4.13.0",
        "inversify-binding-decorators": "4.0.0",
        "inversify-koa-utils": "1.0.0",
        "inversify-logger-middleware": "3.1.0",
        "inversify-socket-utils": "^1.0.1",
        "inversify-tracer": "1.2.0",
        "ioredis": "^4.0.0",
        "jsonwebtoken": "8.3.0",
        "klaw": "3.0.0",
        "koa": "2.5.2",
        "koa-bodyparser": "4.2.1",
        "koa-csrf": "3.0.6",
        "koa-helmet": "^4.0.0",
        "koa-jwt": "3.5.0",
        "koa-log4": "2.3.0",
        "koa-multer": "1.0.2",
        "koa-req-validator": "0.2.1",
        "koa-router": "7.4.0",
        "koa2-cors": "2.0.6",
        "lodash": "4.17.10",
        "lodash-joins": "3.0.0-beta.2",
        "log4js": "3.0.5",
        "moment": "^2.22.2",
        "moment-range": "4.0.1",
        "mysql2": "^1.6.1",
        "reflect-metadata": "^0.1.12",
        "request": "^2.88.0",
        "uuid": "^3.3.2",
        "uuid-with-v6": "1.1.2",
        "validator": "10.7.0"
    },
    "devDependencies": {
        "@types/chai": "4.1.4",
        "@types/ioredis": "^4.0.1",
        "@types/jsonwebtoken": "7.2.8",
        "@types/klaw": "2.1.1",
        "@types/koa": "2.0.46",
        "@types/koa-bodyparser": "5.0.1",
        "@types/koa-helmet": "^3.1.2",
        "@types/koa-router": "7.0.31",
        "@types/koa2-cors": "^2.0.1",
        "@types/lodash": "4.14.116",
        "@types/mocha": "5.2.5",
        "@types/mysql": "^2.15.5",
        "@types/node": "10.9.4",
        "@types/request": "^2.47.1",
        "@types/ws": "^6.0.0",
        "babel-plugin-istanbul": "^5.0.1",
        "chai": "4.1.2",
        "connect": "3.6.6",
        "coveralls": "3.0.2",
        "fury": "3.0.0-beta.7",
        "glob": "7.1.3",
        "gulp": "^4.0.0",
        "husky": "v0.15.0-rc.7",
        "install-peers": "1.0.2",
        "js-beautify": "1.8.1",
        "js-yaml": "3.12.0",
        "jscpd": "0.6.22",
        "lint-staged": "7.2.2",
        "minimatch": "3.0.4",
        "mocha": "5.2.0",
        "mocha-multi-reporters": "1.1.7",
        "mocha-typescript": "^1.1.17",
        "mochawesome": "3.0.3",
        "nodemon": "1.18.4",
        "nyc": "^13.0.1",
        "opener": "1.5.1",
        "source-map-support": "0.5.9",
        "tap-nyc": "^1.0.3",
        "ts-mockito": "2.3.1",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "typedoc": "^0.12.0",
        "typeorm-model-generator": "^0.2.20",
        "typescript": "3.0.3"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "tslint"
        ]
    },
    "nodemonConfig": {
        "ignore": [
            "src/tools/*"
        ]
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "exclude": [
            "**/*.d.ts",
            "src/config/",
            "src/middlewares/",
            "src/sqlTemplate/",
            "src/app.ts",
            ".nyc_output/",
            ".vscode/",
            "config/",
            "coverage/",
            "doc/",
            "logs/",
            "mock/",
            "node_modules/",
            "test/",
            "*.js",
            "*.json",
            "src/models/",
            "src/utils/DaoOperator.ts",
            "src/index.ts",
            "src/ioc/"
        ],
        "reporter": [
            "lcov",
            "text",
            "text-summary"
        ],
        "watermarks": {
            "lines": [
                80,
                95
            ],
            "functions": [
                80,
                95
            ],
            "branches": [
                80,
                95
            ],
            "statements": [
                80,
                95
            ]
        },
        "check-coverage": false,
        "sourceMap": true,
        "cache": false,
        "exclude-after-remap": false,
        "report-dir": "./coverage",
        "instrument": true,
        "all": true
    }
}
